  RowBox[{"%", ",", " ", 
   RowBox[{"(*", " ", 
    RowBox[{"the", " ", "previous", " ", "output"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"hypothesis", "&&", "defW", "&&", "invW", "&&", 
    RowBox[{"(*", " ", 
