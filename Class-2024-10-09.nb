Cell[BoxData[
 RowBox[{
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     RowBox[{"-", 
      FractionBox["1", "\[ExponentialE]"]}], "\[LessEqual]", "x", "<", "0"}], 
    "&&", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"W", "[", "x", "]"}], "\[Equal]", 
       RowBox[{"ProductLog", "[", 
        RowBox[{
         RowBox[{"-", "1"}], ",", "x"}], "]"}]}], "||", 
      RowBox[{
       RowBox[{"W", "[", "x", "]"}], "\[Equal]", 
       RowBox[{"ProductLog", "[", "x", "]"}]}]}], ")"}]}], ")"}], "||", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{"x", ">", "0"}], "&&", 
    RowBox[{
     RowBox[{"W", "[", "x", "]"}], "\[Equal]", 
     RowBox[{"ProductLog", "[", "x", "]"}]}]}], ")"}], "||", 
  RowBox[{"x", "<", 
   RowBox[{
    TagBox[
     SuperscriptBox["W", 
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}]],
     InverseFunction,
